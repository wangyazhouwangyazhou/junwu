.box {
	background-color: #ffffff;
	border-radius: 10rpx;
  font-size: 30rpx;
	color: #000000;
  margin: 10rpx 25rpx;
  padding: 14rpx 15rpx;
  width: 670rpx;
}

.imgs {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  margin-top: 14rpx;
}

.img {
  margin: 0 auto;
}

.position {
  width: 750rpx;
	height: 250rpx;
	background-color: #ffffff;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999;
}

.icon {
  width: 80rpx;
  height: 80rpx;
}

.icon-img {
  display: flex;
  justify-content: space-between;
  padding-top: 11rpx;
}

.submit {
  width: 600rpx;
	height: 50rpx;
	border-radius: 10rpx;
	border: solid 1rpx #46aa40;
  font-size: 30rpx;
	color: #ffffff;
  line-height: 50rpx;
  text-align: center;
}

.btn {
  display: flex;
  justify-content: space-around;
  margin-top: 58rpx;
}

.goodsTitleInput {
	font-size: 32rpx;
	margin: 30rpx 30rpx;
	padding:30rpx;
	background-color: #fff; 
}

.up {
	width: 700rpx;
	height: 50rpx;
	color: #a5a5a5;
	border-radius: 10rpx;
  font-size: 60rpx;
  line-height: 50rpx;
  text-align: center;
  background: #fff;
  margin: 20rpx auto 20rpx auto;
}

.index {
  width: 460rpx;
	height: 140rpx;
  background: #fff;
  position: absolute;
  left: 150rpx;
  top: 470rpx
}